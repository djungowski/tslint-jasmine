{
  "name": "tslint-jasmine",
  "version": "1.0.2",
  "description": "Set of TSLint checkers for jasmine",
  "main": "tslint.json",
  "scripts": {
    "pretest": "npm run build",
    "test": "tslint -r dist --test \"tests/**\"",
    "build": "./bin/build.sh",
    "prepublishOnly": "node -e \"throw 'You should not publish package from root directory. Publish from dist/ instead'\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djungowski/tslint-jasmine.git"
  },
  "keywords": [
    "jasmine",
    "tslint",
    "lint",
    "testing",
    "test",
    "unittest",
    "unit",
    "integration",
    "focus",
    "fdescribe",
    "fit",
    "xdescribe",
    "xit",
    "pending"
  ],
  "author": "Dominik Ehrenberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djungowski/tslint-jasmine/issues"
  },
  "homepage": "https://github.com/djungowski/tslint-jasmine#readme",
  "devDependencies": {
    "@types/jasmine": "^3.4.6",
    "@types/node": "^12.12.7",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
